{
  "name": "my-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --no-web",
    "start:development": "cp .env.development .env && expo start --no-web",
    "start:staging": "cp .env.staging .env && expo start --no-web",
    "start:production": "cp .env.production .env && expo start --no-web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test": "jest --watchAll",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 50",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:development": "cp .env.development .env && eas build --platform all --profile development",
    "build:staging": "cp .env.staging .env && eas build --platform all --profile staging",
    "build:production": "cp .env.production .env && eas build --platform all --profile production",
    "deploy:staging": "eas submit --platform all --profile staging",
    "deploy:production": "eas submit --platform all --profile production",
    "prebuild": "expo prebuild --clean",
    "prebuild:staging": "cp .env.staging .env && expo prebuild --clean",
    "prebuild:production": "cp .env.production .env && expo prebuild --clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "firebase:deploy": "firebase deploy --only functions,firestore:rules,storage",
    "firebase:deploy:staging": "firebase use staging && firebase deploy --only functions,firestore:rules,storage",
    "firebase:deploy:production": "firebase use production && firebase deploy --only functions,firestore:rules,storage",
    "env:development": "cp .env.development .env && echo 'Environment set to development'",
    "env:staging": "cp .env.staging .env && echo 'Environment set to staging'",
    "env:production": "cp .env.production .env && echo 'Environment set to production'"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-firebase/app": "^22.2.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.0.14",
    "@stripe/stripe-react-native": "0.38.6",
    "@types/react-native": "^0.72.8",
    "@types/stripe": "^8.0.416",
    "date-fns": "^4.1.0",
    "expo": "52",
    "expo-barcode-scanner": "^14.0.0",
    "expo-camera": "~16.0.18",
    "expo-constants": "~17.0.8",
    "expo-crypto": "~14.0.2",
    "expo-firebase-recaptcha": "^2.3.1",
    "expo-font": "~13.0.4",
    "expo-image-manipulator": "~13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "firebase": "^11.9.1",
    "firebase-functions": "^6.3.2",
    "react": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.9",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-webview": "13.12.5",
    "stripe": "^17.7.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "codecov": "^3.8.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^9.2.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.0",
    "prettier": "^3.5.3",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
